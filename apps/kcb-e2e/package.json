{
  "name": "kcb-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "kcb"
    ],
    "tags": [
      "scope:kcb",
      "type:app-e2e"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/kcb-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "kcb:build",
          "kcb:serve"
        ]
      }
    }
  }
}
