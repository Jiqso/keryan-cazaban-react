{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "npx nx build kcf --prod && npx nx build kcb --prod",
  "outputDirectory": "dist/apps/kcf",
  "installCommand": "npm install",
  "framework": null,
  "builds": [
    {
      "src": "dist/apps/kcb/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/apps/kcb/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
