{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npx nx run-many -t build --projects=kcf,kcb",
  "outputDirectory": "dist/apps/kcf",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "nodejs20.x"
    }
  }
}
